<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editStory</from-outcome>
			<to-view-id>/storyForm.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>storyView</from-outcome>
			<to-view-id>/storyView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/storyForm.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>storyList</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect include-view-params="true" />
		</navigation-case>
		<navigation-case>
			<from-outcome>chapterForm</from-outcome>
			<to-view-id>/chapterForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/chapterForm.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/storyForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application />
</faces-config>
